{
  "name": "bielik-matura",
  "version": "1.0.0",
  "description": "Asystent matematyczny dla maturzystów - rozwiązywanie zadań maturalnych krok po kroku z użyciem AI",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "mcp-proxy": "node mcp-proxy-server.js",
    "lean-proxy": "node lean-proxy-server.js",
    "rag-service": "cd rag_service && source venv/bin/activate && python main.py",
    "start:proxies": "concurrently \"npm run mcp-proxy\" \"npm run lean-proxy\"",
    "start:all": "concurrently \"npm run dev\" \"npm run mcp-proxy\" \"npm run lean-proxy\" \"npm run rag-service\""
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@types/react-syntax-highlighter": "^15.5.13",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "html2canvas": "^1.4.1",
    "katex": "^0.16.28",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-katex": "^3.1.0",
    "react-syntax-highlighter": "^16.1.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
